package assignment;

public class day2 {
	public static void main(String[] args) {
		int x=55;
		int y=44;
		int z=66;
		
		if(x>y)
		{
			if(x>z)
			{
				System.out.println("X is the greatest");
			}
			else
			{
				System.out.println("Z is the greatest");
			}
		}
		else if (y>z)
		{
			if(y>z)
			{
				System.out.println("Y is the greatest");
			}
			else
			{
				System.out.println("Z is the greatest");
			}
	
		}
	}

}
